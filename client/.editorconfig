# http://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 2
# 换行符lf
end_of_line = lf
# 是否删除行尾的空格
insert_final_newline = true
# 是否在文件的最后插入一个空行
trim_trailing_whitespace = true

[*.md]
# 是否删除行尾的空格
insert_final_newline = false
# 是否在文件的最后插入一个空行
trim_trailing_whitespace = false
